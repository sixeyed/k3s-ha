{
  "cluster": {
    "name": "k3s-test",
    "version": "v1.31.1+k3s1"
  },
  "network": {
    "proxyIP": "10.0.1.100",
    "controlPlaneIPs": [
      "10.0.1.10"
    ],
    "workerIPs": [
      "10.0.1.20"
    ]
  },
  "kubernetes": {
    "serviceCIDR": "10.43.0.0/16",
    "clusterCIDR": "10.42.0.0/16",
    "clusterDNS": "10.43.0.10",
    "clusterDomain": "cluster.local",
    "nodePortRange": "30000-32767",
    "maxPods": 110
  },
  "storage": {
    "device": "/dev/sdb",
    "nfsMountPath": "/data/nfs",
    "nfsProvisionerPath": "/data/nfs/shared"
  },
  "ssh": {
    "user": "ubuntu",
    "keyPath": "~/.ssh/id_rsa"
  },
  "k3s": {
    "token": null,
    "taintControlPlane": false,
    "disableServices": ["traefik"],
    "tlsSans": [],
    "extraArgs": {
      "server": [],
      "agent": []
    }
  },
  "operations": {
    "drainTimeout": 300,
    "upgradeStrategy": "rolling",
    "backupRetention": 7
  }
}