{
  "cluster": {
    "name": "k3s-ha-vagrant",
    "version": "v1.31.1+k3s1"
  },
  "network": {
    "proxyIP": "192.168.56.100",
    "controlPlaneIPs": ["192.168.56.10", "192.168.56.11", "192.168.56.12"],
    "workerIPs": ["192.168.56.20", "192.168.56.21"]
  },
  "kubernetes": {
    "serviceCIDR": "10.43.0.0/16",
    "clusterCIDR": "10.42.0.0/16",
    "clusterDNS": "10.43.0.10",
    "clusterDomain": "cluster.local",
    "nodePortRange": "30000-32767",
    "maxPods": 110
  },
  "storage": {
    "device": "/dev/sdc",
    "nfsMountPath": "/mnt/nfs-storage",
    "nfsProvisionerPath": "/mnt/nfs-storage/shared"
  },
  "ssh": {
    "user": "vagrant",
    "keyPath": "ssh_keys/vagrant_rsa_k3s-control-plane-1"
  },
  "k3s": {
    "token": "k3s-ha-token-887061",
    "taintControlPlane": true,
    "disableServices": ["traefik"],
    "tlsSans": [],
    "extraArgs": {
      "server": [
        "--write-kubeconfig-mode=644",
        "--kube-apiserver-arg=enable-admission-plugins=NodeRestriction"
      ],
      "agent": []
    }
  },
  "operations": {
    "drainTimeout": 300,
    "upgradeStrategy": "rolling",
    "backupRetention": 7
  }
}