{
  "cluster": {
    "name": "k3s-ha-vagrant",
    "version": "v1.31.1+k3s1"
  },
  "network": {
    "proxyIP": "192.168.56.100",
    "masterIPs": ["192.168.56.10", "192.168.56.11", "192.168.56.12"],
    "workerIPs": ["192.168.56.20", "192.168.56.21"]
  },
  "kubernetes": {
    "serviceCIDR": "10.43.0.0/16",
    "clusterCIDR": "10.42.0.0/16",
    "clusterDNS": "10.43.0.10",
    "clusterDomain": "cluster.local",
    "nodePortRange": "30000-32767",
    "maxPods": 110
  },
  "storage": {
    "device": "/dev/sdb",
    "nfsMountPath": "/data/nfs"
  },
  "ssh": {
    "user": "vagrant",
    "keyPath": "~/.ssh/id_rsa"
  },
  "k3s": {
    "token": null,
    "disableServices": ["traefik"],
    "tlsSans": [],
    "extraArgs": {
      "server": [],
      "agent": []
    }
  },
  "operations": {
    "drainTimeout": 300,
    "upgradeStrategy": "rolling",
    "backupRetention": 7
  }
}